<p id="notice"><%= notice %></p>

<h1>Listing Eventos</h1>

<table>
  <thead>
    <tr>
      <th>Dt evento</th>
      <th>Dt inicio</th>
      <th>Dt termino</th>
      <th>Created by</th>
      <th>Update by</th>
      <th>Obs</th>
      <th>Tp evento</th>
      <th>Status</th>
      <th>Sinc google</th>
      <th>Dt sinc</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @eventos.each do |evento| %>
      <tr>
        <td><%= evento.dt_evento %></td>
        <td><%= evento.dt_inicio %></td>
        <td><%= evento.dt_termino %></td>
        <td><%= evento.created_by %></td>
        <td><%= evento.update_by %></td>
        <td><%= evento.obs %></td>
        <td><%= evento.tp_evento %></td>
        <td><%= evento.status %></td>
        <td><%= evento.sinc_google %></td>
        <td><%= evento.dt_sinc %></td>
        <td><%= link_to 'Show', evento %></td>
        <td><%= link_to 'Edit', edit_evento_path(evento) %></td>
        <td><%= link_to 'Destroy', evento, method: :delete, data: { confirm: 'Confirma a ExclusÃ£o?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

